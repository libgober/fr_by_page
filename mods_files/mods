         <titleInfo>
              <title>Oil Pollution Act of 1990 Phase-out Requirements for Single Hull Tank Vessels</title>
              <partNumber>Part VI</partNumber>
              <partName>Proposed Rules</partName>
        </titleInfo>
         <relatedItem type="otherFormat" xlink:href="https://www.govinfo.gov/content/pkg/FR-2000-01-18/html/00-1028.htm">
              <identifier type="FDsys Unique ID">D09002ee1c82050e0</identifier>
