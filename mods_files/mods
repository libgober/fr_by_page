         <genre authority="marcgt">article</genre>
         <titleInfo>
              <title>Disapproval of Implementation Plans; California State Implementation Plan Revision, South Coast Air Quality Management District</title>
              <partName>Proposed Rules</partName>
        </titleInfo>
         <relatedItem type="otherFormat" xlink:href="https://www.govinfo.gov/content/pkg/FR-2000-01-18/html/00-1090.htm">
              <identifier type="FDsys Unique ID">D09002ee1c82050dd</identifier>
        </relatedItem>
         <relatedItem type="otherFormat" xlink:href="https://www.govinfo.gov/content/pkg/FR-2000-01-18/pdf/00-1090.pdf">
              <identifier type="FDsys Unique ID">D09002ee1c82051fb</identifier>
