    <relatedItem type="constituent" ID="id-00-1000" xlink:href="https://www.govinfo.gov/metadata/granule/FR-2000-01-18/00-1000/mods.xml">
         <genre authority="marcgt">article</genre>
         <titleInfo>
              <title>Office of Special Education and Rehabilitative Services; National Institute on Disability and Rehabilitation; Research Notice Inviting Applications for a New Award for a Rehabilitation Research and Training Center for Fiscal Year (FY) 2000</title>
              <partNumber>Part III</partNumber>
              <partName>Notices</partName>
        </titleInfo>
         <relatedItem type="otherFormat" xlink:href="https://www.govinfo.gov/content/pkg/FR-2000-01-18/html/00-1000.htm">
              <identifier type="FDsys Unique ID">D09002ee1c8205197</identifier>
