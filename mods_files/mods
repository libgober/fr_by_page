    <relatedItem type="constituent" ID="id-00-1039" xlink:href="https://www.govinfo.gov/metadata/granule/FR-2000-01-18/00-1039/mods.xml">
         <genre authority="marcgt">article</genre>
         <titleInfo>
              <title>Agency Information Collection Activities: Proposed Collection; Comment Request</title>
              <partName>Notices</partName>
        </titleInfo>
         <relatedItem type="otherFormat" xlink:href="https://www.govinfo.gov/content/pkg/FR-2000-01-18/html/00-1039.htm">
